{
    "name": "lucasandrade/laravel-jwtauth",
    "description": "Biblioteca de autenticação usando JWT",
    "keywords": ["framework", "laravel", "JWT"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.1.*",
        "illuminate/database": "5.1.*",
        "illuminate/http": "5.1.*",
        "firebase/php-jwt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "orchestra/testbench": "~3.0",
        "laravel/framework": "5.1.*"
    },
    "autoload": {
        "psr-4":{
            "Lucandrade\\JwtAuth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lucas Andrade de Oliveira",
            "email": "lucas.andrade.oliveira@hotmail.com"
        }
    ],
    "config":{
        "bin-dir": "bin/"
    },
    "minimum-stability": "dev",
    "scripts": {
        "test":        "bin/phpunit -c phpunit.xml"
    }
}